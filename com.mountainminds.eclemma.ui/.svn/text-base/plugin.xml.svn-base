<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin>
   <extension
         point="org.eclipse.core.runtime.adapters">
      <factory
            adaptableType="com.mountainminds.eclemma.core.ICoverageSession"
            class="com.mountainminds.eclemma.internal.ui.WorkbenchAdapterFactory">
         <adapter type="org.eclipse.ui.model.IWorkbenchAdapter"/>
      </factory>
   </extension>
   <extension
         point="org.eclipse.debug.ui.launchGroups">
      <launchGroup
            bannerImage="$nl$/icons/full/wizban/run_coverage.gif"
            id="com.mountainminds.eclemma.ui.launchGroup.coverage"
            image="$nl$/icons/full/elcl16/runcoverage.gif"
            label="%CoverageLaunchGroup.label"
            mode="coverage"
            title="%CoverageLaunchGroup.title"/>
   </extension>
   <extension
         point="org.eclipse.core.expressions.propertyTesters">
      <propertyTester
            class="com.mountainminds.eclemma.internal.ui.actions.ContextualLaunchableTester"
            id="com.mountainminds.eclemma.ui.propertyTester.contextualLaunchable"
            namespace="com.mountainminds.eclemma.ui"
            properties="launchable"
            type="java.lang.Object"/>
   </extension>
   <extension
         point="org.eclipse.debug.ui.launchShortcuts">
      <shortcut
            label="%JavaApplicationShortcut.label"
            icon="$nl$/icons/full/etool16/java_launch.gif"
            modes="coverage"
            class="com.mountainminds.eclemma.ui.launching.CoverageLaunchShortcut:org.eclipse.jdt.debug.ui.localJavaShortcut"
            id="com.mountainminds.eclemma.ui.localJavaShortcut">
         <contextualLaunch>
           <enablement>
             <with variable="selection">
               <test property="com.mountainminds.eclemma.ui.launchable" args="org.eclipse.jdt.debug.ui.localJavaShortcut"/>
             </with>
           </enablement>
  		 </contextualLaunch>
      </shortcut>
      <shortcut
            label="%JUnitTestShortcut.label"
            icon="$nl$/icons/full/etool16/junit_launch.gif"
            class="com.mountainminds.eclemma.ui.launching.CoverageLaunchShortcut:org.eclipse.jdt.junit.junitShortcut"
            modes="coverage"
            id="com.mountainminds.eclemma.ui.junitShortcut">
         <contextualLaunch>
           <enablement>
             <with variable="selection">
               <test property="com.mountainminds.eclemma.ui.launchable" args="org.eclipse.jdt.junit.junitShortcut"/>
             </with>
           </enablement>
         </contextualLaunch>
      </shortcut>
      <shortcut
            label="%EclipseApplicationShortcut.label"
            icon="$nl$/icons/full/etool16/eclipse_launch.gif"
            modes="coverage"
            class="com.mountainminds.eclemma.ui.launching.CoverageLaunchShortcut:org.eclipse.pde.ui.runtimeWorkbenchShortcut"
            id="com.mountainminds.eclemma.ui.workbenchShortcut">
         <contextualLaunch>
           <enablement>
             <with variable="selection">
               <test property="com.mountainminds.eclemma.ui.launchable" args="org.eclipse.pde.ui.runtimeWorkbenchShortcut"/>
             </with>
           </enablement>
         </contextualLaunch>         
      </shortcut>
      <shortcut
            label="%JUnitPluginTestShortcut.label"
            icon="$nl$/icons/full/etool16/junitplugin_launch.gif"
            modes="coverage"
            class="com.mountainminds.eclemma.ui.launching.CoverageLaunchShortcut:org.eclipse.pde.ui.junitWorkbenchShortcut"
            id="com.mountainminds.eclemma.ui.junitPluginShortcut">
         <contextualLaunch>
           <enablement>
             <with variable="selection">
               <test property="com.mountainminds.eclemma.ui.launchable" args="org.eclipse.pde.ui.junitWorkbenchShortcut"/>
             </with>
           </enablement>
         </contextualLaunch>
      </shortcut>
      <shortcut
            label="%JUnitRAPTestShortcut.label "
            icon="$nl$/icons/full/etool16/junitplugin_launch.gif"
            modes="coverage"
            class="com.mountainminds.eclemma.ui.launching.CoverageLaunchShortcut:org.eclipse.rap.ui.launch.RAPJUnitLaunchShortcut"
            id="com.mountainminds.eclemma.ui.RAPJUnitLaunchShortcut">
         <contextualLaunch>
            <enablement>
               <with variable="selection">
                  <test property="com.mountainminds.eclemma.ui.launchable" args="org.eclipse.rap.ui.launch.RAPJUnitLaunchShortcut"/>
               </with>
            </enablement>
         </contextualLaunch>
      </shortcut>
      <shortcut
            class="com.mountainminds.eclemma.ui.launching.CoverageLaunchShortcut:org.eclipse.swtbot.eclipse.ui.junitShortcut"
            icon="$nl$/icons/full/etool16/swtbot_launch.gif"
            id="com.mountainminds.eclemma.ui.SWTBotJUnitShortcut"
            label="%SWTBotJUnitShortcut.label"
            modes="coverage">
         <contextualLaunch>
            <enablement>
               <with
                     variable="selection">
                  <test
                        args="org.eclipse.swtbot.eclipse.ui.junitShortcut"
                        property="com.mountainminds.eclemma.ui.launchable">
                  </test>
               </with>
            </enablement>
         </contextualLaunch>
      </shortcut>      
   </extension>
   
   <extension
         point="org.eclipse.ui.actionSets">
      <actionSet
            id="com.mountainminds.eclemma.ui.CoverageActionSet"
            label="%CoverageActionSet.label"
            visible="false">
         <menu
               id="org.eclipse.ui.run"
               label="%RunMenu.label"
               path="additions">
            <separator name="relaunchGroup"/>
            <separator name="emptyLaunchGroup"/>
         </menu>
         <action
               class="com.mountainminds.eclemma.internal.ui.actions.CoverageToolbarAction"
               disabledIcon="$nl$/icons/full/dlcl16/runcoverage.gif"
               hoverIcon="$nl$/icons/full/elcl16/runcoverage.gif"
               icon="$nl$/icons/full/elcl16/runcoverage.gif"
               id="com.mountainminds.eclemma.ui.actions.CoverageDropDownAction"
               label="%CoverageDropDownAction.label"
               style="pulldown"
               toolbarPath="org.eclipse.debug.ui.launchActionSet/debug"/>
         <action
               class="com.mountainminds.eclemma.internal.ui.actions.OpenCoverageConfigurations"
               definitionId="com.mountainminds.eclemma.ui.commands.OpenCoverageConfiguration"
               id="com.mountainminds.eclemma.ui.actions.OpenCoverageConfiguration"
               label="%OpenCoverageConfigurationsAction.label"
               menubarPath="org.eclipse.ui.run/emptyLaunchGroup"/>
         <action
               class="com.mountainminds.eclemma.internal.ui.actions.CoverageAsAction"
               id="com.mountainminds.eclemma.ui.actions.CoverageAsAction"
               label="%CoverageAsAction.label"
               menubarPath="org.eclipse.ui.run/emptyLaunchGroup"
               style="pulldown"/>
         <action
               class="com.mountainminds.eclemma.internal.ui.actions.CoverageHistoryAction"
               id="com.mountainminds.eclemma.ui.actions.CoverageHistoryAction"
               label="%CoverageHistoryAction.label"
               menubarPath="org.eclipse.ui.run/emptyLaunchGroup"
               style="pulldown"/>
         <action
               class="com.mountainminds.eclemma.internal.ui.actions.CoverageLastAction"
               definitionId="com.mountainminds.eclemma.ui.commands.CoverageLast"
               disabledIcon="$nl$/icons/full/dlcl16/relaunch.gif"
               hoverIcon="$nl$/icons/full/elcl16/relaunch.gif"
               icon="$nl$/icons/full/elcl16/relaunch.gif"
               id="com.mountainminds.eclemma.ui.actions.CoverageLast"
               label="%CoverageLastAction.label"
               menubarPath="org.eclipse.ui.run/relaunchGroup"/>
      </actionSet>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <command
            categoryId="org.eclipse.debug.ui.category.run"
            description="%CoverageLastCommand.description"
            id="com.mountainminds.eclemma.ui.commands.CoverageLast"
            name="%CoverageLastCommand.name"/>
      <command
            categoryId="org.eclipse.debug.ui.category.run"
            description="%OpenCoverageConfigurationsCommand.description"
            id="com.mountainminds.eclemma.ui.commands.OpenCoverageConfiguration"
            name="%OpenCoverageConfigurationsCommand.name"/>
      <command
            categoryId="org.eclipse.debug.ui.category.run"
            description="%CoverageJavaApplicationCommand.description"
            id="com.mountainminds.eclemma.ui.localJavaShortcut.coverage"
            name="%CoverageJavaApplicationCommand.name"/>
      <command
            categoryId="org.eclipse.debug.ui.category.run"
            description="%CoverageJUnitTestCommand.description"
            id="com.mountainminds.eclemma.ui.junitShortcut.coverage"
            name="%CoverageJUnitTestCommand.name"/>
      <command
            categoryId="org.eclipse.debug.ui.category.run"
            description="%CoverageEclipseApplicationCommand.description"
            id="com.mountainminds.eclemma.ui.workbenchShortcut.coverage"
            name="%CoverageEclipseApplicationCommand.name"/>
      <command
            categoryId="org.eclipse.debug.ui.category.run"
            description="%CoverageJUnitPluginTestCommand.description"
            id="com.mountainminds.eclemma.ui.junitPluginShortcut.coverage"
            name="%CoverageJUnitPluginTestCommand.name"/>
      <command
            categoryId="org.eclipse.debug.ui.category.run"
            description="%CoverageJUnitRAPTestCommand.description"
            id="com.mountainminds.eclemma.ui.junitRAPShortcut.coverage"
            name="%CoverageJUnitRAPTestCommand.name">
      </command>
      <command
            categoryId="org.eclipse.debug.ui.category.run"
            description="%CoverageSWTBotJUnitCommand.description"
            id="com.mountainminds.eclemma.ui.swtBotJunitShortcut.coverage"
            name="%CoverageSWTBotJUnitCommand.name">
      </command>
   </extension>
   
   <extension
         point="org.eclipse.ui.bindings">
      <key
            commandId="com.mountainminds.eclemma.ui.localJavaShortcut.coverage"
            contextId="org.eclipse.ui.globalScope"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="M3+M2+E J"/>
      <key
            commandId="com.mountainminds.eclemma.ui.junitShortcut.coverage"
            contextId="org.eclipse.ui.globalScope"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="M3+M2+E T"/>
      <key
            commandId="com.mountainminds.eclemma.ui.workbenchShortcut.coverage"
            contextId="org.eclipse.ui.globalScope"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="M3+M2+E E"/>
      <key
            commandId="com.mountainminds.eclemma.ui.junitPluginShortcut.coverage"
            contextId="org.eclipse.ui.globalScope"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="M3+M2+E P"/>
      <key
            commandId="com.mountainminds.eclemma.ui.junitRAPShortcut.coverage"
            contextId="org.eclipse.ui.globalScope"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="M3+M2+E R"/>
      <key
            commandId="com.mountainminds.eclemma.ui.swtBotJunitShortcut.coverage"
            contextId="org.eclipse.ui.globalScope"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="M3+M2+E S">
      </key>
      <key
            commandId="com.mountainminds.eclemma.ui.commands.CoverageLast"
            contextId="org.eclipse.ui.contexts.window"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="M1+M2+F11"/>
      <!-- override key binding on Mac OS X -->
      <key
            contextId="org.eclipse.ui.contexts.window"
            platform="carbon"
            sequence="M1+M2+F11"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"/>
      <key
            contextId="org.eclipse.ui.contexts.window"
            platform="cocoa"
            sequence="M1+M2+F11"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"/>
      <key
            commandId="com.mountainminds.eclemma.ui.commands.CoverageLast"
            platform="carbon"
            contextId="org.eclipse.ui.contexts.window"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="M2+M4+F11"/>
      <key
            commandId="com.mountainminds.eclemma.ui.commands.CoverageLast"
            platform="cocoa"
            contextId="org.eclipse.ui.contexts.window"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="M2+M4+F11"/>
   </extension>
   
   <extension
         point="org.eclipse.ui.perspectiveExtensions">
      <perspectiveExtension targetID="org.eclipse.jdt.ui.JavaPerspective">
         <actionSet id="com.mountainminds.eclemma.ui.CoverageActionSet"/>
         <showInPart id="com.mountainminds.eclemma.ui.CoverageView"/>
      </perspectiveExtension>
      <perspectiveExtension targetID="org.eclipse.debug.ui.DebugPerspective">
         <actionSet id="com.mountainminds.eclemma.ui.CoverageActionSet"/>
         <showInPart id="com.mountainminds.eclemma.ui.CoverageView"/>
      </perspectiveExtension>
      <perspectiveExtension targetID="org.eclipse.pde.ui.PDEPerspective">
         <actionSet id="com.mountainminds.eclemma.ui.CoverageActionSet"/>
         <showInPart id="com.mountainminds.eclemma.ui.CoverageView"/>
      </perspectiveExtension>
      <perspectiveExtension targetID="org.eclipse.jst.j2ee.J2EEPerspective">
         <actionSet id="com.mountainminds.eclemma.ui.CoverageActionSet"/>
         <showInPart id="com.mountainminds.eclemma.ui.CoverageView"/>
      </perspectiveExtension>
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <view
            allowMultiple="false"
            category="org.eclipse.jdt.ui.java"
            class="com.mountainminds.eclemma.internal.ui.coverageview.CoverageView"
            icon="$nl$/icons/full/eview16/coverage.gif"
            id="com.mountainminds.eclemma.ui.CoverageView"
            name="%coverageView.title"/>
   </extension>
