   </extension>
   <extension
         point="org.eclipse.ui.propertyPages">
      <page
            class="com.mountainminds.eclemma.internal.ui.dialogs.CoveragePropertyPage"
            id="com.mountainminds.eclemma.ui.coveragePropertyPage"
            name="%coveragePropertyPage.title"
            objectClass="org.eclipse.jdt.core.IJavaElement"/>
   </extension>
   <extension
         point="org.eclipse.ui.decorators">
      <decorator
            adaptable="false"
            class="com.mountainminds.eclemma.internal.ui.decorators.CoverageDecorator"
            id="com.mountainminds.eclemma.ui.coverageDecorator"
            label="%coverageDecorator.label"
            lightweight="true"
            state="false">
         <description>%coverageDecorator.description</description>
         <enablement>
            <objectClass
                  name="org.eclipse.jdt.core.IJavaElement">
            </objectClass>
         </enablement>
      </decorator>
   </extension>
   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            category="org.eclipse.jdt.ui.preferences.JavaBasePreferencePage"
            class="com.mountainminds.eclemma.internal.ui.dialogs.CoveragePreferencePage"
            id="com.mountainminds.eclemma.ui.preferencePages.CoverageGeneral"
            name="%coveragePreferences.title">
         <keywordReference id="com.mountainminds.eclemma.ui.coveragePreferences"/>
      </page>
   </extension>
   <extension
         point="org.eclipse.ui.keywords">
      <keyword
            id="com.mountainminds.eclemma.ui.coveragePreferences"
            label="%keywords.coveragePreferences"/>
   </extension>
   <extension
         point="org.eclipse.ui.popupMenus">
