<?xml version="1.0" encoding="UTF-8"?>
<feature
      id="org.revisionfilter.feature.eclemma"
      label="Eclemma SVN Coverage Filter"
      version="1.0.0.2">

   <description>
      Add option to Coverage View to enable a filter which excludes coverage of code which is unchanged according to SVN.
   </description>

   <requires>
      <import plugin="org.eclipse.jdt.core" version="3.5.0" match="greaterOrEqual"/>
      <import plugin="com.mountainminds.eclemma.core" version="1.4.2" match="greaterOrEqual"/>
      <import plugin="com.mountainminds.eclemma.ui" version="1.4.2" match="greaterOrEqual"/>
      <import plugin="org.revisionfilter.utils" version="1.0.0" match="greaterOrEqual"/>
   </requires>

   <plugin
         id="org.revisionfilter.eclemma"
         download-size="0"
         install-size="0"
         version="1.0.0.2"
         fragment="true"
         unpack="false"/>

</feature>
